<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_product_category_tree_inherit_studio_fields" model="ir.ui.view">
      <field name="name">product.category.tree.inherit.studio.fields</field>
      <field name="model">product.category</field>
      <field name="inherit_id" ref="product.product_category_list_view"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='display_name']" position="after">
          <field name="property_cost_method"/>
          <field name="property_valuation"/>
          <field name="property_account_creditor_price_difference_categ"/>
          <field name="property_account_income_categ_id"/>
          <field name="property_account_expense_categ_id"/>
          <field name="property_stock_valuation_account_id"/>
        </xpath>
      </field>
    </record>
</odoo>
